    "version": "0.1.0",
    "description": "Yggdrasil grammar for tree-sitter",
    "main": "bindings/node",
    "keywords": [
        "parser",
        "rust"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/nyar-vm/Yggdrasil-Grammar"
    },
