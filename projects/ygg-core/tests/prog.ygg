grammar Json5 {}

entry union Value {
    | Object  #Object
    | Array   #Array
    | String  #String
    | Number  #Number
    | Boolean #Boolean
    | Null    #Null
}

class Object {
    '{' Value? '}'
}

class Array {
    '[' Value? ']'
}

atomic class String {
    | "'" "'"
    | "\"" "\""
}

atomic class Number {
    [1-9][0-9]
}

union Boolean {
    | "true"  #True
    | "false" #False
}

class Null {
    "null"
}

ignore class WhiteSpace {
    UNICODE_WHITE_SPACE
}